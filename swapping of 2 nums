#include<iostream>
using namespace std;
int main()
{
	int x,y;
	cout<<"enter x";
	cin>>x;
	cout<<"enter y";
	cin>>y;
	cout<<"before swapping"<<endl;
	cout<<"x="<<x<<"y="<<y<<endl;
	int swap;
	swap=x;
	x=y;
	y=swap;
    cout<<"after swapping:"<<endl;
    cout<<"x="<<x<<"y="<<y<<endl;
    return 0;
}

