#include<iostream>
using namespace std;
int main()
{
	int a,b,c,d;
	a=10;
	b=5;
	c=70;
	d=35;
	if(a>b && a>c && a>d)
	{
		cout<<"a is largest";
	}
	else if(b>a && b>c && b>d)
	{
		cout<<"b is largest";
	}
	else if(c>a && c>b && c>d)
	{
		cout<<"c is largest";
	}
	else
	{
		cout<<"d is largest";
	}
	return 0;
}
