#include<iostream>
using namespace std;
int main()
{
	int originalnumber,r,sum=0,n;
	cout<<"enter the number :";
	cin>>n;
	originalnumber=n;
	while(n>0)
	{
		r=n%10;
		sum=sum+(r*r*r);
	    n=n/10;
	}
	if(sum==originalnumber)
	{
		cout<<"the number is armstrong number";
	}
	else
	{
		cout<<"the numbner is not an armstrong number";
	}
	return 0;
}
